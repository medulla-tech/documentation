#: ../../source/usemedulla/glpi/index.rst:183
msgid "def xml_fix(xml):"
msgstr ""

#: ../../source/usemedulla/glpi/index.rst:184
msgid "import xml.etree.cElementTree as ET"
msgstr ""

#: ../../source/usemedulla/glpi/index.rst:185
msgid "xml = ET.fromstring(xml)"
msgstr ""

#: ../../source/usemedulla/glpi/index.rst:186
msgid "tree = ET.ElementTree(xml)"
msgstr ""

#: ../../source/usemedulla/glpi/index.rst:187
msgid "root = tree.getroot()"
msgstr ""

#: ../../source/usemedulla/glpi/index.rst:188
msgid "for subelem1 in root:"
msgstr ""

#: ../../source/usemedulla/glpi/index.rst:189
msgid "if subelem1.tag == 'CONTENT':"
msgstr ""

#: ../../source/usemedulla/glpi/index.rst:190
msgid "for subelem2 in subelem1:"
msgstr ""

#: ../../source/usemedulla/glpi/index.rst:191
msgid "for subelem3 in subelem2:"
msgstr ""

#: ../../source/usemedulla/glpi/index.rst:193
msgid "# DELL vendor name should always be the same"
msgstr ""

#: ../../source/usemedulla/glpi/index.rst:194
msgid ""
"if subelem3.text in ['DELL', 'Dell Corp.', 'Dell Computer Corp.', 'Dell',"
" 'Dell Computer Corporation']:"
msgstr ""

#: ../../source/usemedulla/glpi/index.rst:195
msgid "subelem3.text = 'Dell Inc.'"
msgstr ""

#: ../../source/usemedulla/glpi/index.rst:197
msgid "return ET.tostring(root)"
msgstr ""

